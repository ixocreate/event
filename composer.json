{
    "name": "ixocreate/event",
    "description": "IXOCREATE event integration",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "IXOCREATE"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "ixocreate/contract": "dev-develop",
        "ixocreate/servicemanager": "dev-develop",
        "symfony/event-dispatcher": "^4.1"
    },
    "require-dev": {
        "ixocreate/coding-standard": "^1.0",
        "phpunit/phpunit": "^6.0.6",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Ixocreate\\Event\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IxocreateTest\\Event\\": "tests/src/",
            "IxocreateMisc\\Event\\": "tests/misc/"
        }
    },
    "scripts": {
        "fix-cs": "vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --config vendor/ixocreate/coding-standard/.php_cs"
    }
}
