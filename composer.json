{
    "name": "kiwi-suite/event",
    "description": "kiwi-suite/event",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Thomas Payer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "kiwi-suite/contract": "dev-develop",
        "symfony/event-dispatcher": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0.6",
        "friendsofphp/php-cs-fixer": "^2.0",
        "kiwi-suite/application": "dev-develop",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "KiwiSuite\\Event\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KiwiSuiteTest\\Event\\": "tests/src/",
            "KiwiSuiteMisc\\Event\\": "tests/misc/"
        }
    }
}
