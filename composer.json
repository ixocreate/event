{
    "name": "ixocreate/event",
    "description": "IXOCREATE event integration",
    "license": "MIT",
    "type": "library",
    "authors": [{
        "name": "IXOCREATE"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "ixocreate/contract": "dev-develop",
        "symfony/event-dispatcher": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0.6",
        "friendsofphp/php-cs-fixer": "^2.0",
        "ixocreate/application": "dev-develop",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Ixocreate\\Event\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IxocreateTest\\Event\\": "tests/src/",
            "IxocreateMisc\\Event\\": "tests/misc/"
        }
    }
}
